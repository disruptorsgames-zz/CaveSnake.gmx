        <arguments>
          <argument>
            <kind>1</kind>
            <string>/// Player Movement

// motion controls
if (os_type == os_ios)
{
    // accelerometer
    dx = device_get_tilt_x() * spd * accel;
    dy = device_get_tilt_y() * spd * accel;
    dz = device_get_tilt_z() * spd * accel;
    
    switch (display_get_orientation())
    {
        // reverse of y for landscape
        case display_landscape:
            physics_apply_force(x, y, -dy, 0);
            break;
        case display_portrait:
            physics_apply_force(x, y, dx, 0);
            break;
    }
    
    // jump
    if (mouse_check_button_pressed(mb_left) &amp;&amp; onGround(phy_position_x, phy_position_y))
    {
        playSound(sfx_jump);
        physics_apply_impulse(x, y, 0, -spd / 2);
    }
}
// keyboard movement
else
{
    // level 2 sideways movement
    if (sideways)
    {
        // up and down
        if (keyboard_check(global.JUMP))
            physics_apply_force(x, y, 0, -spd);
        if (keyboard_check(global.DOWN))
            physics_apply_force(x, y, 0, spd);
            
        // jump
        if (keyboard_check_pressed(global.LEFT) &amp;&amp; onGround(phy_position_x, phy_position_y))
        {
            playSound(sfx_jump);
            physics_apply_impulse(x, y, -spd / 2, 0);
        }
    }
    else
    {
        // left and right
        if (keyboard_check(global.LEFT))
            physics_apply_force(x, y, -spd, 0);
        if (keyboard_check(global.RIGHT))
            physics_apply_force(x, y, spd, 0);
        
        // jump
        if (keyboard_check_pressed(global.JUMP) &amp;&amp; onGround(phy_position_x, phy_position_y))
        {
            playSound(sfx_jump);
            physics_apply_impulse(x, y, 0, -spd / 2);
        }
    }
}
</string>
