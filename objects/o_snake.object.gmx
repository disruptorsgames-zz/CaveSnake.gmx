    phy_position_x += 5;

    // force movement
    if (keyboard_check(vk_up))
        physics_apply_force(x, y, 0, -20);
    if (keyboard_check(vk_down))
        physics_apply_force(x, y, 0, 20);
}
else
{
    // force movement
    if (keyboard_check(vk_left))
        physics_apply_force(x, y, -20, 0);
    if (keyboard_check(vk_right))
        physics_apply_force(x, y, 20, 0);
    
    // impulse movement
    if (keyboard_check_pressed(vk_up) &amp;&amp; phy_linear_velocity_y &gt; -0.1 &amp;&amp; phy_linear_velocity_y &lt; 0.1)
    {
        audio_play_sound(sfx_jump, 10, false);
        physics_apply_impulse(x, y, 0, -12);
    }
}
    
instance_create(x, y, o_tail);
</string>
